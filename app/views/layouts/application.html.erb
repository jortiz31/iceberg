<!DOCTYPE html>
<html>
<head>
  <title>Iceberg</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="row">
    <div class="medium-7 medium-centered columns">
      <% if alert.present? %>
      <div class="panel radius">
        <%= alert %>
      </div>
      <% end %>

    <% if notice.present? %>
      <div class="panel callout radius">
        <%= notice %>
      </div>
      <% end %>
    </div>
  </div>






<%= yield %>



</body>
</html>
